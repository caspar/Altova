<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.bookstore.org" targetNamespace="http://www.bookstore.org" elementFormDefault="qualified" version="1.1">
	<xs:override schemaLocation="Book.xsd">
		<xs:element name="Review" type="xs:error"/>
	</xs:override>
	<xs:element name="BookStore">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Book" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>

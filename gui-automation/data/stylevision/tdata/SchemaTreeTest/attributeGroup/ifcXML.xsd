				<xsd:element minOccurs="0" name="targetUsers">
					<xsd:complexType>
						<xsd:choice maxOccurs="unbounded">
							<xsd:group ref="ActorSelect"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>

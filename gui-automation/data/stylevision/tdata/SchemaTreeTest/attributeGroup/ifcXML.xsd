<xsd:complexType abstract="true" name="Property">
	<xsd:complexContent>
		<xsd:extension base="element">
			<xsd:sequence>
				<xsd:element type="Identifier" name="name"/>
				<xsd:element type="Text" minOccurs="0" name="description"/>
			</xsd:sequence>
		</xsd:extension>
	</xsd:complexContent>
</xsd:complexType>

<xsd:complexType name="PropertyBoundedValue">
	<xsd:complexContent>
		<xsd:extension base="SimpleProperty">
			<xsd:sequence>
				<xsd:element name="upperBoundValue">
					<xsd:complexType>
						<xsd:choice>
							<xsd:group ref="Value"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="lowerBoundValue">
					<xsd:complexType>
						<xsd:choice>
							<xsd:group ref="Value"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element minOccurs="0" name="unit">
					<xsd:complexType>
						<xsd:choice>
							<xsd:group ref="Unit"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:extension>
	</xsd:complexContent>
</xsd:complexType>

<xsd:complexType abstract="true" name="PropertyDefinition">
	<xsd:complexContent>
		<xsd:extension base="Root">
		</xsd:extension>
	</xsd:complexContent>
</xsd:complexType>

<xsd:complexType name="PropertyEnumeratedValue">
	<xsd:complexContent>
		<xsd:extension base="SimpleProperty">
			<xsd:sequence>
				<xsd:element name="enumerationValues">
					<xsd:complexType>
						<xsd:choice maxOccurs="unbounded">
							<xsd:group ref="Value"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:extension>
	</xsd:complexContent>
</xsd:complexType>

<xsd:complexType name="PropertyEnumeration">
	<xsd:complexContent>
		<xsd:extension base="element">
			<xsd:sequence>
				<xsd:element type="Label" name="name"/>
				<xsd:element name="enumerationValues">
					<xsd:complexType>
						<xsd:choice maxOccurs="unbounded">
							<xsd:group ref="Value"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element minOccurs="0" name="unit">
					<xsd:complexType>
						<xsd:choice>
							<xsd:group ref="Unit"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:extension>
	</xsd:complexContent>
</xsd:complexType>

<xsd:complexType name="PropertySet">
	<xsd:complexContent>
		<xsd:extension base="PropertySetDefinition">
			<xsd:sequence>
				<xsd:element name="hasProperties">
					<xsd:complexType>
						<xsd:choice maxOccurs="unbounded" minOccurs="1">
							<xsd:element ref="Property"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:extension>
	</xsd:complexContent>
</xsd:complexType>

<xsd:complexType abstract="true" name="PropertySetDefinition">
	<xsd:complexContent>
		<xsd:extension base="PropertyDefinition">
		</xsd:extension>
	</xsd:complexContent>
</xsd:complexType>

<xsd:complexType name="PropertySingleValue">
	<xsd:complexContent>
		<xsd:extension base="SimpleProperty">
			<xsd:sequence>
				<xsd:element name="nominalValue">
					<xsd:complexType>
						<xsd:choice>
							<xsd:group ref="Value"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element minOccurs="0" name="unit">
					<xsd:complexType>
						<xsd:choice>
							<xsd:group ref="Unit"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:extension>
	</xsd:complexContent>
</xsd:complexType>

<xsd:complexType name="PropertyTableValue">
	<xsd:complexContent>
		<xsd:extension base="SimpleProperty">
			<xsd:sequence>
				<xsd:element name="definingValues">
					<xsd:complexType>
						<xsd:choice maxOccurs="unbounded">
							<xsd:group ref="Value"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="definedValues">
					<xsd:complexType>
						<xsd:choice maxOccurs="unbounded">
							<xsd:group ref="Value"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element type="Text" minOccurs="0" name="expression"/>
				<xsd:element minOccurs="0" name="definingUnit">
					<xsd:complexType>
						<xsd:choice>
							<xsd:group ref="Unit"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element minOccurs="0" name="definedUnit">
					<xsd:complexType>
						<xsd:choice>
							<xsd:group ref="Unit"/>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:extension>
	</xsd:complexContent>
</xsd:complexType>


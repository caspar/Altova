<?xml version="1.0" encoding="UTF-8"?><structure version="7" cssmode="strict" relativeto="*SPS" encodinghtml="UTF-8" encodingrtf="ISO-8859-1" encodingpdf="UTF-8" embed-images="1">	<parameters/>	<schemasources>		<namespaces/>		<schemasources>			<xsdschemasource name="$XML" main="1" schemafile="Data.xsd" workingxmlfile="Data.xml">				<xmltablesupport/>				<textstateicons/>			</xsdschemasource>		</schemasources>	</schemasources>	<modules/>	<flags>		<scripts/>		<globalparts/>		<designfragments/>		<pagelayouts/>	</flags>	<scripts>		<script language="javascript"/>	</scripts>	<globalstyles>		<rules selector=".info">			<media>				<media value="all"/>			</media>			<rule background-color="#f6f6ff" border="1px solid navy" color="navy" font-weight="bold" margin-bottom="12px" margin-top="12px" padding="2px"/>		</rules>		<rules selector=".explanation">			<media>				<media value="all"/>			</media>			<rule color="blue" font-style="italic" margin-bottom="12px"/>		</rules>	</globalstyles>	<mainparts>		<children>			<globaltemplate match="/" matchtype="named" parttype="main">				<children>					<paragraph paragraphtag="h2">						<styles border-bottom="2px solid navy" color="navy"/>						<children>							<text fixtext="Example: Input Formatting"/>						</children>					</paragraph>					<paragraph paragraphtag="p">						<properties class="info"/>						<children>							<text fixtext="This example shows how you can format numbers and dates in StyleVision. This format is applied in Authentic and, except for certain formatting field types and options, in XSLT as well."/>						</children>					</paragraph>					<paragraph paragraphtag="p">						<properties class="explanation"/>						<children>							<text fixtext="Input formatting is applied by right clicking on a (content) element, and choosing &quot;Edit Input Formatting...&quot; from the context menu."/>						</children>					</paragraph>					<template match="$XML" matchtype="schemasource">						<editorproperties elementstodisplay="5"/>						<children>							<template match="data" matchtype="schemagraphitem">								<editorproperties elementstodisplay="5"/>								<children>									<paragraph paragraphtag="p">										<properties class="explanation"/>										<children>											<text fixtext="Date formatting:"/>										</children>									</paragraph>									<template match="@last-edited" matchtype="schemagraphitem">										<editorproperties elementstodisplay="5"/>										<children>											<table>												<properties border="1" width="100%"/>												<children>													<tablebody>														<children>															<tablerow>																<styles background-color="#e9ffe9" font-weight="bold"/>																<children>																	<tablecell>																		<children>																			<text fixtext="Style"/>																		</children>																	</tablecell>																	<tablecell>																		<children>																			<text fixtext="Format String"/>																		</children>																	</tablecell>																	<tablecell>																		<children>																			<text fixtext="Result"/>																		</children>																	</tablecell>																</children>															</tablerow>															<tablerow>																<children>																	<tablecell>																		<children>																			<text fixtext="unformatted"/>																		</children>																	</tablecell>																	<tablecell>																		<children>																			<text fixtext="(none)"/>																		</children>																	</tablecell>																	<tablecell>																		<children>																			<content>																				<format datatype="date"/>																			</content>																		</children>																	</tablecell>																</children>															</tablerow>															<tablerow>																<children>																	<tablecell>																		<children>																			<text fixtext="US style"/>																		</children>																	</tablecell>																	<tablecell>																		<children>																			<text fixtext="MM / DD / YYYY"/>																		</children>																	</tablecell>																	<tablecell>																		<children>																			<content>																				<format string="MM / DD / YYYY" datatype="date"/>																			</content>																		</children>																	</tablecell>																</children>															</tablerow>															<tablerow>																<children>																	<tablecell>																		<children>																			<text fixtext="German style"/>																		</children>																	</tablecell>																	<tablecell>																		<children>																			<text fixtext="DD. MM. YYYY"/>																		</children>																	</tablecell>																	<tablecell>																		<children>																			<content>																				<format string="DD. MM. YYYY" datatype="date"/>																			</content>																		</children>																	</tablecell>																</children>															</tablerow>															<tablerow>																<children>																	<tablecell>																		<children>																			<text fixtext="Japanese style"/>																		</children>																	</tablecell>																	<tablecell>																		<children>																			<text fixtext="YYYY&apos;年&apos;MM&apos;月&apos;DD&apos;日&apos;"/>																		</children>																	</tablecell>																	<tablecell>																		<children>																			<content>																				<format string="YYYY&apos;年&apos;MM&apos;月&apos;DD&apos;日&apos;" datatype="date"/>																			</content>																		</children>																	</tablecell>																</children>															</tablerow>															<tablerow>																<children>																	<tablecell>																		<children>																			<text fixtext="Long style"/>																		</children>																	</tablecell>																	<tablecell>																		<children>																			<text fixtext="Weekday&apos;,&apos; Month DD YYYY"/>																		</children>																	</tablecell>																	<tablecell>																		<children>																			<content>																				<format string="Weekday&apos;,&apos; Month DD YYYY" datatype="date"/>																			</content>																		</children>																	</tablecell>																</children>															</tablerow>														</children>													</tablebody>												</children>											</table>										</children>									</template>									<paragraph paragraphtag="p">										<properties class="explanation"/>										<children>											<text fixtext="Number formatting:"/>										</children>									</paragraph>									<template match="numbers" matchtype="schemagraphitem">										<editorproperties elementstodisplay="5"/>										<children>											<template match="@total" matchtype="schemagraphitem">												<editorproperties elementstodisplay="5"/>												<children>													<table>														<properties border="1" width="100%"/>														<children>															<tablebody>																<children>																	<tablerow>																		<styles background-color="#e9ffe9" font-weight="bold"/>																		<children>																			<tablecell>																				<children>																					<text fixtext="Style"/>																				</children>																			</tablecell>																			<tablecell>																				<children>																					<text fixtext="Format String"/>																				</children>																			</tablecell>																			<tablecell>																				<children>																					<text fixtext="Result"/>																				</children>																			</tablecell>																		</children>																	</tablerow>																	<tablerow>																		<children>																			<tablecell>																				<children>																					<text fixtext="unformatted"/>																				</children>																			</tablecell>																			<tablecell>																				<children>																					<text fixtext="(none)"/>																				</children>																			</tablecell>																			<tablecell>																				<children>																					<content>																						<format datatype="decimal"/>																					</content>																				</children>																			</tablecell>																		</children>																	</tablerow>																	<tablerow>																		<children>																			<tablecell>																				<children>																					<text fixtext="with decimal separators and 2 fraction digits"/>																				</children>																			</tablecell>																			<tablecell>																				<children>																					<text fixtext="###,##0.00"/>																				</children>																			</tablecell>																			<tablecell>																				<children>																					<content>																						<format string="###,##0.00" datatype="decimal"/>																					</content>																				</children>																			</tablecell>																		</children>																	</tablerow>																	<tablerow>																		<children>																			<tablecell>																				<children>																					<text fixtext="with decimal separators, 9 fixed places and 2 fraction digits"/>																				</children>																			</tablecell>																			<tablecell>																				<children>																					<text fixtext="000,000,000.00"/>																				</children>																			</tablecell>																			<tablecell>																				<children>																					<content>																						<format string="000,000,000.00" datatype="decimal"/>																					</content>																				</children>																			</tablecell>																		</children>																	</tablerow>																</children>															</tablebody>														</children>													</table>												</children>											</template>										</children>									</template>									<paragraph paragraphtag="p">										<properties class="explanation"/>										<children>											<text fixtext="AutoCalc formatting: The result of an XPath calculation can be formatted as well (although it can never be editable), the only difference being that since the resultant data type cannot be statically evaluated, you have to specify it in the input formatting dialog."/>										</children>									</paragraph>									<table>										<properties border="1" width="100%"/>										<children>											<tablebody>												<children>													<tablerow>														<styles background-color="#e9ffe9" font-weight="bold"/>														<children>															<tablecell>																<children>																	<text fixtext="Style"/>																</children>															</tablecell>															<tablecell>																<children>																	<text fixtext="Format String"/>																</children>															</tablecell>															<tablecell>																<children>																	<text fixtext="Result"/>																</children>															</tablecell>														</children>													</tablerow>													<tablerow>														<children>															<tablecell>																<children>																	<text fixtext="dateTime"/>																</children>															</tablecell>															<tablecell>																<children>																	<text fixtext="MM / DD / YYYY hh:mm:ss.sss"/>																</children>															</tablecell>															<tablecell>																<children>																	<autocalc xpath="&quot;2008-11-30T15:21:13.48&quot;">																		<format string="MM / DD / YYYY hh:mm:ss.sss" datatype="dateTime"/>																	</autocalc>																</children>															</tablecell>														</children>													</tablerow>													<tablerow>														<children>															<tablecell>																<children>																	<text fixtext="duration"/>																</children>															</tablecell>															<tablecell>																<children>																	<text fixtext="*Y &apos;yr,&apos; *M &apos;mth,&apos; *D &apos;dy,&apos; *h &apos;hr,&apos; *m &apos;min,&apos; *s.s* &apos;sec&apos;"/>																</children>															</tablecell>															<tablecell>																<children>																	<autocalc xpath="&quot;P12M3DT14H3.47S&quot;">																		<format string="*Y &apos;yr,&apos; *M &apos;mth,&apos; *D &apos;dy,&apos; *h &apos;hr,&apos; *m &apos;min,&apos; *s.s* &apos;sec&apos;" datatype="duration"/>																	</autocalc>																</children>															</tablecell>														</children>													</tablerow>													<tablerow>														<children>															<tablecell>																<children>																	<text fixtext="gYearMonth"/>																</children>															</tablecell>															<tablecell>																<children>																	<text fixtext="&quot;month &quot;MM&quot; of &quot; YYYY"/>																</children>															</tablecell>															<tablecell>																<children>																	<autocalc xpath="&quot;2008-11&quot;">																		<format string="&quot;month &quot;MM&quot; of &quot; YYYY" datatype="gYearMonth"/>																	</autocalc>																</children>															</tablecell>														</children>													</tablerow>												</children>											</tablebody>										</children>									</table>								</children>							</template>						</children>					</template>				</children>			</globaltemplate>		</children>	</mainparts>	<globalparts/>	<pagelayout/>	<designfragments/></structure>
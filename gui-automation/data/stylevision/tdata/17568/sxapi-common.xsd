	<xs:import namespace="http://www.govtalk.gov.uk/core" schemaLocation="CommonSimpleTypes.xsd">
		<xs:annotation>
			<xs:documentation>
				GovTalk schema for general data type definitions
			</xs:documentation>
		</xs:annotation>
	</xs:import>
	<xs:import namespace="http://www.govtalk.gov.uk/people/PersonDescriptives" schemaLocation="PersonDescriptiveTypes.xsd">
		<xs:annotation>
			<xs:documentation>
				Person descriptive types architectural schema.
				Includes GovTalk schema for general data type definitions 
				(CommonSimpleTypes-v1-3.xsd).
			</xs:documentation>
		</xs:annotation>
	</xs:import>
	<xs:import namespace="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" schemaLocation="CitizenIdentificationTypes.xsd">
		<xs:annotation>
			<xs:documentation>
				Citizen Identifaction Types architectural schema.
				Includes GovTalk schema for general data type definitions 
				(CommonSimpleTypes-v1-3.xsd).
			</xs:documentation>
		</xs:annotation>
	</xs:import>
	<xs:import namespace="http://www.govtalk.gov.uk/people/bs7666" schemaLocation="BS7666.xsd">
		<xs:annotation>
			<xs:documentation>
				Schema for British Standard BS7666 address types
			</xs:documentation>
		</xs:annotation>
	</xs:import>
	<!-- xs:import namespace="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" schemaLocation="AddressTypes.xsd" -->
	<!-- xs:import namespace="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" schemaLocation="ContactTypes.xsd" -->
	<!-- xs:import namespace="http://www.govtalk.gov.uk/financial/GDSCFinancialTypes" schemaLocation="FinancialTypes.xsd" -->

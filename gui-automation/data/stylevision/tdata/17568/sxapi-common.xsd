	<xs:simpleType name="SxapiIdentifier.type">
		<xs:restriction base="xs:nonNegativeInteger">
			<xs:totalDigits value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiYesNo.type">
		<xs:annotation>
			<xs:documentation>Y->yes, N->no</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:minLength value="1" fixed="false"/>
			<xs:enumeration value="yes"/>
			<xs:enumeration value="no"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiYesNoUnknown.type">
		<xs:annotation>
			<xs:documentation>Y->yes, N->no, U->unknown</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:minLength value="1"/>
			<xs:maxLength value="7"/>
			<xs:enumeration value="yes"/>
			<xs:enumeration value="no"/>
			<xs:enumeration value="unknown"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiTextOrList.type">
		<xs:annotation>
			<xs:documentation>Assessment question answer type: T=text, L=list </xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:enumeration value="T"/>
			<xs:enumeration value="L"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiTermCode.type">
		<xs:annotation>
			<xs:documentation>S->short term, L->long term or U->undetermined</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:minLength value="9"/>
			<xs:enumeration value="short term"/>
			<xs:enumeration value="long term"/>
			<xs:enumeration value="undetermined"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiPlacementTerm.type">
		<xs:annotation>
			<xs:documentation>S->short term, L->long term, A->agreement, n->n/a</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:enumeration value="short term"/>
			<xs:enumeration value="long term"/>
			<xs:enumeration value="agreement"/>
			<xs:enumeration value="n/a"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiPaymentType.type">
		<xs:annotation>
			<xs:documentation>G->gross, N->net</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:enumeration value="gross"/>
			<xs:enumeration value="net"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiActionStatus.type">
		<xs:annotation>
			<xs:documentation>O->open, C->closed</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:enumeration value="open"/>
			<xs:enumeration value="closed"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiContactReferral.type">
		<xs:annotation>
			<xs:documentation>C=contact, R=referral</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation>contact</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R">
				<xs:annotation>
					<xs:documentation>referral</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiRoleInvolvement.type">
		<xs:annotation>
			<xs:documentation>EndInv or EndRole or None - amended AR 02/05/2005</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="7"/>
			<xs:minLength value="6"/>
			<xs:enumeration value="EndInv"/>
			<xs:enumeration value="EndRole"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiDate.type">
		<xs:restriction base="core:DateType"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiTime.type">
		<xs:restriction base="xs:time"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiDateTime.type">
		<xs:annotation>
			<xs:documentation>ISO 8601 CCYY-MM-DDThh:mm:ss</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiCode.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiMeaning.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="240"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiNotes.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="2000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiSearchPattern.type">
		<xs:annotation>
			<xs:documentation>Free text for pattern matching</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="240"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiUser.type">
		<xs:annotation>
			<xs:documentation>Oracle identifier</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiTable.type">
		<xs:annotation>
			<xs:documentation>Oracle identifier</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiVersion.type">
		<xs:annotation>
			<xs:documentation>
				New-style version type to replace checksums.
				Allows for up to 8-character prefix and 128-character hash.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="136"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiTrueFalse.type">
		<xs:restriction base="xs:boolean">
			<xs:pattern value="true"/>
			<xs:pattern value="false"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiCheckSum.type">
		<xs:restriction base="xs:unsignedLong">
			<xs:maxInclusive value="18446744073709551615"/>
			<xs:minInclusive value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiNameSurname.type">
		<xs:annotation>
			<xs:documentation>if length(persons.family_name)>35 then truncate</xs:documentation>
		</xs:annotation>
		<xs:restriction base="pdt:PersonFamilyNameType"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiNameTitle.type">
		<xs:annotation>
			<xs:documentation>if length(coded_titles.meaning)>35 then truncate</xs:documentation>
		</xs:annotation>
		<xs:restriction base="pdt:PersonNameTitleType"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiNameForename.type">
		<xs:annotation>
			<xs:documentation>if length(persons.first_names)>35 then truncate</xs:documentation>
		</xs:annotation>
		<xs:restriction base="pdt:PersonGivenNameType"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiNameList.type">
		<xs:annotation>
			<xs:documentation>For example, "Catlin Ivor(Catlin Rakesh, Ivor   Raman Test Mr) (35)"</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiPersonName.type">
		<xs:annotation>
			<xs:documentation>NameTitle(40)+1+Forename(40)+1+Surname(40)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="122"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiInitials.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiSex.type">
		<xs:annotation>
			<xs:documentation>GENF->2, GENM->1, GENI->9, GENU->0</xs:documentation>
		</xs:annotation>
		<xs:restriction base="pdt:GenderCurrentType"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiAge.type">
		<xs:annotation>
			<xs:documentation>years - Modified AR 12/05/2006 for heat 270935</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:unsignedInt"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiMonths.type">
		<xs:annotation>
			<xs:documentation>months + days</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="12"/>
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiPersonOrFamily.type">
		<xs:annotation>
			<xs:documentation>P->person, F->family</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:minLength value="6"/>
			<xs:enumeration value="person"/>
			<xs:enumeration value="family"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiContactDetails.type">
		<xs:annotation>
			<xs:documentation>e.g., related persons contact by phone, e-mail, fax, letter or in person</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiScore.type">
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="12"/>
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiNumberOfUnits.type">
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="12"/>
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiHealthId.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiNationalInsuranceNumber.type">
		<xs:annotation>
			<xs:documentation>length 30->9</xs:documentation>
		</xs:annotation>
		<xs:restriction base="apd:NationalInsuranceNumberType"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiReferenceIdentifier.type">
		<xs:annotation>
			<xs:documentation>Reference identifier the person is known by</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiReferenceDescription.type">
		<xs:annotation>
			<xs:documentation>Free text description of the system the reference relates to</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiReferenceTypeCode.type">
		<xs:annotation>
			<xs:documentation>Internal code grouping external reference types EXTERNAL_REFERENCE_TYPES.CODE</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiReferenceTypeName.type">
		<xs:annotation>
			<xs:documentation>Name of the type of external reference  EXTERNAL_REFERENCE_TYPES.TYPE_NAME</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="40"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiReferenceTypeDescription.type">
		<xs:annotation>
			<xs:documentation>Description of the type of external reference  EXTERNAL_REFERENCE_TYPES.TYPE_DESCRIPTION</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiServiceDescription.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="765"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiCareItemDescription.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="726"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiReviewerName.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="62"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiProviderName.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="322"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiCreditorName.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiHomeOfficeNumber.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiOrganisationName.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiOrganisationShortName.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiOrganisationStandardName.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="273"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiOrganisationRegistrationNumber.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiOrganisationAtAddress.type">
		<xs:annotation>
			<xs:documentation>Name of organisation at address (e.g. Police)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="60"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiDepartmentAtAddress.type">
		<xs:annotation>
			<xs:documentation>Department within organisation based at this address</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="60"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiFileId.type">
		<xs:annotation>
			<xs:documentation>The file identifier for the [paper] file</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiSubject.type">
		<xs:annotation>
			<xs:documentation>The subject of the paper file</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiLocationText.type">
		<xs:annotation>
			<xs:documentation>Free text notes relating to the location of the file</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiTelephoneNumber.type">
		<xs:annotation>
			<xs:documentation>length 35->20</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:TelephoneNumberType"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiTelephoneExtension.type">
		<xs:annotation>
			<xs:documentation>length 5->6</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:TelephoneExtensionType"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiEmailAddress.type">
		<xs:annotation>
			<xs:documentation>length 100->127</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:EmailAddressType"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiWebAddress.type">
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiCountry.type">
		<xs:annotation>
			<xs:documentation>e.g., CountryOfBirth</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiPlace.type">
		<xs:annotation>
			<xs:documentation>e.g., PlaceOfBirth</xs:documentation>
		</xs:annotation>
		<xs:restriction base="core:PopulatedStringType">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SxapiLocality.type">
		<xs:annotation>
			<xs:documentation>length 50->35 as in BS7666</xs:documentation>
		</xs:annotation>
		<xs:restriction base="bs:LocalityType"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiTown.type">
		<xs:annotation>
			<xs:documentation>length 50->30 as in BS7666</xs:documentation>
		</xs:annotation>
		<xs:restriction base="bs:TownType"/>
	</xs:simpleType>
	<xs:simpleType name="SxapiAdminArea.type">
